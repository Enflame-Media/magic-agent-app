{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./sources/*"
      ],
      "@react-navigation/native-stack": [
        "./node_modules/expo-router/node_modules/@react-navigation/native-stack"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "sources/trash/**/*",
    "sources/dev/fixtures/**/*"
  ]
}